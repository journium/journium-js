{
  "name": "@journium/core",
  "version": "1.0.4",
  "license": "MIT",
  "description": "Core functionality and types for Journium SDK - identity, events, and configuration",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://github.com/journium/journium-js",
  "bugs": {
    "url": "https://github.com/journium/journium-js/issues"
  },
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --passWithNoTests",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "uuidv7": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rollup": "^4.55.1",
    "@rollup/plugin-typescript": "^12.3.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "rollup-plugin-dts": "^6.3.0",
    "tslib": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/journium/journium-js.git",
    "directory": "packages/core"
  },
  "keywords": [
    "journium",
    "core",
    "analytics",
    "identity",
    "events",
    "types",
    "typescript",
    "sdk"
  ],
  "author": "Journium Team",
  "publishConfig": {
    "access": "public"
  }
}