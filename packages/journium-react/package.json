{
  "name": "@journium/react",
  "version": "1.1.1",
  "type": "module",
  "license": "MIT",
  "description": "Journium React integration - Provider, hooks, and components for React applications",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://github.com/journium/journium-js",
  "bugs": {
    "url": "https://github.com/journium/journium-js/issues"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --passWithNoTests",
    "test:compat": "cd test-compat && ./test-all.sh",
    "test:compat:16.8": "cd test-compat && ./test-react-16.8.sh",
    "test:compat:16.14": "cd test-compat && ./test-react-16.14.sh",
    "test:compat:17": "cd test-compat && ./test-react-17.sh",
    "test:compat:18": "cd test-compat && ./test-react-18.sh",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@journium/js": "workspace:*",
    "@journium/core": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/node": "^20.0.0",
    "react": "^18.0.0",
    "rollup": "^3.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "rollup-plugin-dts": "^5.0.0",
    "tslib": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/journium/journium-js.git",
    "directory": "packages/journium-react"
  },
  "keywords": [
    "analytics",
    "tracking",
    "journium",
    "react",
    "hooks",
    "provider",
    "context",
    "web-analytics",
    "typescript"
  ],
  "author": "Journium Team",
  "publishConfig": {
    "access": "public"
  }
}